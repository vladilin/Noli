script/source = "class_name time_sys extends Node

@export var datetime: date_time

func _ready():
	if datetime == null:
		datetime = date_time.new()   # always have a resource, even if not set in editor

@export var ticks_pr_sec: int = 6

func _process(delta: float) -> void:
	datetime.increase_by_sec(delta * ticks_pr_sec)
"

